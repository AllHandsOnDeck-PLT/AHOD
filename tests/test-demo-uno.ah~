when do series<Card> CREATEDECK(series<Card> deck):
{ 
    series<string> types 
    string type 
    types = ["R0", "R1", "R2", "R3", "R4"]
    deck.pop() 
    for (i = 0; i < types.size(); i = i + 1):
    {
        type = types[i]
        card = Card(type, true)   
        deck.push(card) 
    }
    return deck
} 
