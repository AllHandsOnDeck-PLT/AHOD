Player player

let Player(string name) be:
{
  string name: name
  int score: 0

  when do PLAY(int number):
  {
  	do PRINT(number)
  }
}

main:
{

  player = Player("bob")

  do PRINT(player.name)
  player do PLAY(3)
}

